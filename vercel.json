{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
		{
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' *"
        }
      ]
    }
  ]
}